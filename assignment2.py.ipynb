    "def fib(n):\n",
    "    # Write code here\n",
    "    #base\n",
    "    if n < 0:\n",
    "        return 0\n",
    "    if n <= 1:\n",
    "        return n\n",
    "    else:\n",
    "        return (fib(n-1) + fib(n-2))\n",
    "\n",
    "fib(4)"
   ]
